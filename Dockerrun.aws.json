{
   "AWSEBDockerrunVersion": 2,
   "containerDefinitions": [
     {
	   "name": "client",
	   "image": "amitdewangan/multi-client",
	   "hostname": "client",
	   "essential": false
	 },
	 {
	   "name": "server",
	   "image": "amitdewangan/multi-server",
	   "hostname": "api",
	   "essential": "false"
	 },
	 {
	   "name": "worker",
	   "image": "amitdewangan/multi-worker",
	   "hostname": "worker",
	   "essential": "false"
	 },
	 {
	   "name": "nginx",
	   "image": "amitdewangan/multi-nginx",
	   "hostname": "nginx",
	   "essential": true,
	   "portMappings": [
	     {
		   "hostPort": 80,
		   "containerPort": 80  
		 }
	   ],
	   "links": ["client", "server"]
	 }
   ]
}